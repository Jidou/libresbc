{%- for profilename, sipprofile in sipprofiles.items() %}        <profile name="{{profilename}}">
          <gateways>
{%- for gateway in sipprofile.gateways %}  
            <gateway name="{{gateway.name}}">
              <param name="realm" value="{{gateway.realm}}"/>
              <param name="username" value="{{gateway.username}}"/>
              <param name="password" value="{{gateway.password}}"/>
              <param name="from-user" value="{{gateway.from_user}}"/>
              <param name="from-domain" value="{{gateway.from_domain}}"/>
              <param name="proxy" value="{{gateway.ip}}:{{gateway.port}};transport={{gateway.transport}}"/>
              <param name="register" value="{{gateway.register}}"/>
              <param name="register-proxy" value="{{gateway.register_proxy}}"/>
              <param name="caller-id-in-from" value="{{gateway.caller_id_in_from}}"/>
              <param name="extension-in-contact" value="true"/>
              <param name="register-transport" value="{{gateway.transport}}"/>
              <param name="retry-seconds" value="{{gateway.retry_seconds}}" />
              <param name="expire-seconds" value="{{gateway.expire_seconds}}" />
              <param name="ping" value="{{gateway.ping}}"/>
              <param name="ping-max" value="{{gateway.ping_max}}"/>
              <param name="ping-min" value="{{gateway.ping_min}}"/>
              <param name="cid-type" value="{{gateway.sip_cid_type}}"/>
              <param name="extension-in-contact" value="false"/>
            </gateway>
{%- endfor %}
          </gateways>
          <settings>
            <param name="debug" value="0"/>
            <param name="sip-trace" value="yes"/>
            <param name="sip-capture" value="no"/>
            <param name="shutdown-on-fail" value="true"/>
            {# <!-- UA IDENTITY--> #}
            <param name="user-agent-string" value="{{sipprofile.user_agent}}"/>
            <param name="username" value="libresbc"/> {# <!-- sdp username in o= and s= fields --> #}
            {# <!-- AUTH AND SECURE--> #}
            <param name="apply-inbound-acl" value="{{profilename|upper}}ACL"/>
            <param name="auth-calls" value="true"/>
            <param name="nonce-ttl" value="60"/>
            <param name="log-auth-failures" value="true"/>
            <param name="challenge-realm" value="auto_to"/>
            {# <!-- NAT--> #}
            <param name="local-network-acl" value="{{sipprofile.local_network_acl|default('rfc1918.auto')}}"/>
            {# <!-- SIP VERBOSE CALLFLOW CONTROL--> #}
            <param name="enable-100rel" value="{{sipprofile.enable_100rel|default('false')}}"/>
            <param name="ignore-183nosdp" value="true"/>
            <param name="sip-options-respond-503-on-busy" value="{{sipprofile.sip_options_respond_503_on_busy}}"/>
            <param name="disable-register" value="true"/>
            <param name="disable-transfer" value="{{sipprofile.disable_transfer}}"/> {# <!-- call transfer and call forward setting --> #}
            <param name="manual-redirect" value="{{sipprofile.manual_redirect}}"/>
            <param name="enable-3pcc" value="{{sipprofile.enable_3pcc}}"/>
            <param name="enable-compact-headers" value="{{sipprofile.enable_compact_headers}}"/> {# <!-- Do not send UPDATE after session established --> #}
            <param name="send-display-update" value="false"/> 
            <param name="inbound-proxy-media" value="false"/> {# <!-- disable proxy media, thinking to support bypass in future --> #}
            {# <!--  
              'generous' permits the remote codec list have precedence and 'win' the codec negotiation and selection process
              'greedy' forces a win by the local FreeSWITCH preference list
              'scrooge' the FreeSWITCH wins even when the far side lies about capabilities during the negotiation process
            --> #}
            <param name="inbound-codec-negotiation" value="generous"/>
            <param name="inbound-late-negotiation" value="true"/> {# <!-- outbound profile need to be in late negotiation --> #}
            <param name="disable-hold" value="true"/> {# <!-- turn of music-on-hold feature --> #}
            {#<!-- <param name="hold-music" value="$${hold_music}"/> -->#}
            <param name="proxy-hold" value="true"/> {# <!-- re-INVITE for hold/unhold is proxied to other end --> #}
            <param name="suppress-cng" value="true"/> {# <!-- Not use cng at the moment --> #}
            <param name="pass-callee-id" value="false" /> {# <!-- Disable sending X-FS-Support header --> #}
            <param name="stun-enabled" value="false"/> {# <!-- disable stun --> #}
            <param name="t38-passthru" value="true"/> {# <!-- t38 passthrough as sbc not support fax application --> #}
            <param name="dtmf-type" value="{{sipprofile.dtfm_type}}"/>
            <param name="pass-rfc2833" value="true"/>
            <param name="rtp-digit-delay" value="20"/>
            <param name="rtp-timer-name" value="soft"/>
            <param name="media_timeout" value="{{sipprofile.media_timeout}}"/> {# <!-- media_hold_timeout -->}
            <param name="rtp-rewrite-timestamps" value="{{sipprofile.rtp_rewrite_timestamps}}"/> {# <!-- rtp_rewrite_timestamps chanvar in a dialplan, If you don't want to pass through timestamps from 1 RTP call to another (on a per call basis with rtp_rewrite_timestamps chanvar)--> #}
            {# <!-- SIP SESSION TIMER--> #}
            <param name="enable-timer" value="{{sipprofile.enable_timer}}"/>
            <param name="session-timeout" value="{{sipprofile.session_timeout}}"/>
            <param name="minimum-session-expires" value="{{sipprofile.minimum_session_expires}}"/>
            {# <!-- DIALPLAN--> #}
            <param name="dialplan" value="XML"/>
            <param name="context" value="{{sipprofile.context}}"/>
            <param name="dbname" value="/dev/shm/libresbc/sip_{{sipprofile.name}}.db"/>
            {# <!-- SERVER BINDING --> #}
            <param name="rtp-ip" value="{{ netaliases[sipprofile.rtp_address][NODEID]['listen'] }}"/>
            <param name="sip-ip" value="{{ netaliases[sipprofile.sip_address][NODEID]['listen'] }}"/>
            <param name="ext-rtp-ip" value="autonat:{{ netaliases[sipprofile.rtp_address][NODEID]['advertise'] }}"/>
            <param name="ext-sip-ip" value="autonat:{{ netaliases[sipprofile.sip_address][NODEID]['advertise'] }}"/>
            <param name="sip-port" value="{{sipprofile.sip_listen_port}}"/>
            <param name="tls" value="{{sipprofile.sip_tls}}"/>
            <param name="tls-only" value="{{sipprofile.tls_only}}"/>
            <param name="tls-bind-params" value="transport=tls"/>
            <param name="tls-sip-port" value="{{sipprofile.sips_port}}"/>
            {# <!--<param name="tls-cert-dir" value="{{sipprofile.tls_cert_dir}}"/> --> #}
            <param name="tls-verify-date" value="true"/>
            <param name="tls-version" value="tlsv1.2"/>
            <param name="tls-ciphers" value="ALL:!ADH:!LOW:!EXP:!MD5:@STRENGTH"/>
            <param name="tls-verify-policy" value="none"/>          
          </settings>
        </profile>
{%- endfor %}

