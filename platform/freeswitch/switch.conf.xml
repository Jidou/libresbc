    <!--- SWITCH.CONF.XML -->
    <!-- FREESWITCH DEFAULT CONFIGURATION -->
    <configuration name="switch.conf" description="Core Configuration">
      <settings>
        <param name="max-sessions" value="6000"/>
        <param name="sessions-per-second" value="100"/>
        <param name="rtp-start-port" value="$${rtp_start_port}"/>
        <param name="rtp-end-port" value="$${rtp_end_port}"/>
        <param name="core-db-name" value="$${db_location}/fs_core.db" />
      </settings>
    </configuration>