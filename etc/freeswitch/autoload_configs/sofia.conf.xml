    <!--- SOFIA.CONF.XML -->
    <!--- BEGIN: SOFIA SIP SETTING -->  
    <configuration name="sofia.conf" description="sofia Endpoint">
      <global_settings>
        <param name="log-level" value="0"/>
        <param name="debug-presence" value="0"/>
      </global_settings>
      <profiles>
        <X-PRE-PROCESS cmd="comment" data="DYNAMIC ACL, PROVISIONED BY HTTPAPI"/>
        <X-PRE-PROCESS cmd="exec" data="wget -qO - $${configuration_server}/sofia/profile"/>
      </profiles>
    </configuration>
    <!--- END: SOFIA SIP SETTING -->  